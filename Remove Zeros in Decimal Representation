long long removeZeros(long long n)
{
    int dig,i=0;
    long int res=0;
    while(n>0)
    {
        dig=n%10;
        if(dig!=0)
            res=(res*10)+dig;
        n/=10;
    }
    n=res;
    res=0;
    while(n>0)
    {
        dig=n%10;
        if(dig!=0)
            res=(res*10)+dig;
        n/=10;
    }
    return res;
}
