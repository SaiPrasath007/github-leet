int mostWordsFound(char** sentences, int sentencesize)
{
    int j,i,r,mw=0;
    for(i=0;i<sentencesize;i++)
    {
        r=0;
        for(j=0;j<strlen(sentences[i]);j++)
        {
            if(sentences[i][j]==' ')
                r++;
        }
        if(mw<r+1)
            mw=r+1;
    }
    return mw;
}
