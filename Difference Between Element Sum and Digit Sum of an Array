int differenceOfSum(int* nums, int numsize)
{
   int es=0,ds=0,dig;
   for(int i=0;i<numsize;i++)
   {
        es+=nums[i];
        if(nums[i]<10)
            ds+=nums[i];
        else
        {
            while(nums[i]>0)
            {
                dig=nums[i]%10;
                ds+=dig;
                nums[i]/=10;
            }
        }
   }
   return abs(es-ds);
}
