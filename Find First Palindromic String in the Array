char* firstPalindrome(char** words, int wordsize)
{
    int i,j,check=1;
    char w[101];
    for(i=0;i<wordsize;i++)
    {
        w[0]='\0';
        for(j=strlen(words[i])-1;j>=0;j--)
            strncat(w,&words[i][j],1);
        if(strcmp(w,words[i])==0)
        {
            check=0;
            break;
        }
    }
    return (check==0?words[i]:"");
}
